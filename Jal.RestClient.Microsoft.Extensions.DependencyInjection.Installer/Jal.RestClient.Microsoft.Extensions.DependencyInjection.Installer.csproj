  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Jal.HttpClient.Microsoft.Extensions.DependencyInjection.Installer" Version="3.1.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Jal.RestClient\Jal.RestClient.csproj" />
  </ItemGroup>

</Project>
