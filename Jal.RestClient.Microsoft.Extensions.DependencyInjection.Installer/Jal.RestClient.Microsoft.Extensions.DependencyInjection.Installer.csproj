    <Description>Microsoft.Extensions.DependencyInjection installer for the Jal.RestClient library</Description>
    <Authors>Raul Naupari</Authors>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>2.0.0</Version>
    <PackageLicenseUrl />
